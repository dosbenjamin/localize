{
  "extends": "@localize/tsconfig/base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@localize/web/features/*": ["features/*"],
      "@localize/web/libs/*": ["libs/*"],
      "@localize/web/shared/*": ["shared/*"],
    },
    "noEmit": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
